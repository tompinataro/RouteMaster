{
  "compilerOptions": {
    "target": "ES2020",
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "outDir": "dist",
    "rootDir": ".",
    "noEmit": false,
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": [
    "server/**/*.ts"
  ],
  "exclude": [
    "server/__tests__/**",
    "**/*.spec.ts",
    "node_modules",
    "dist"
  ],
  "extends": "expo/tsconfig.base"
}
